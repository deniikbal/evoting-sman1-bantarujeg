{
  "name": "codeguide-starter-fullstack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && node scripts/clean-cache.js",
    "build:local": "next build",
    "pages:build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:export": "tsx scripts/export-sqlite-to-sql.ts",
    "d1:create": "wrangler d1 create evoting-db",
    "d1:create:prod": "wrangler d1 create evoting-db-production",
    "d1:migrate": "wrangler d1 execute evoting-db --file=./drizzle/0000_fixed_starfox.sql",
    "d1:migrate:prod": "wrangler d1 execute evoting-db-production --file=./drizzle/0000_fixed_starfox.sql --env=production",
    "d1:seed": "tsx scripts/seed-d1.ts",
    "d1:list": "wrangler d1 list",
    "d1:info": "wrangler d1 info evoting-db",
    "d1:info:prod": "wrangler d1 info evoting-db-production",
    "d1:check": "wrangler d1 execute evoting-db --command=\"SELECT name FROM sqlite_master WHERE type='table' ORDER BY name\"",
    "d1:check:remote": "wrangler d1 execute evoting-db --remote --command=\"SELECT name FROM sqlite_master WHERE type='table' ORDER BY name\"",
    "d1:data-only": "npm run db:export && wrangler d1 execute evoting-db --file=./drizzle/data-export.sql",
    "d1:sync": "npm run db:export && wrangler d1 execute evoting-db --file=./drizzle/data-export.sql",
    "d1:sync:remote": "npm run db:export && wrangler d1 execute evoting-db --remote --file=./drizzle/0000_fixed_starfox.sql && wrangler d1 execute evoting-db --remote --file=./drizzle/data-export.sql",
    "d1:sync:prod": "npm run db:export && npm run d1:migrate:prod && wrangler d1 execute evoting-db-production --file=./drizzle/data-export.sql --env=production",
    "cf:login": "wrangler login",
    "cf:deploy": "echo 'Please use Cloudflare Dashboard for deployment. See DEPLOY_TO_CLOUDFLARE.md'",
    "docker:build": "docker build -t codeguide-starter-fullstack .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/better-sqlite3": "^7.6.13",
    "better-auth": "^1.3.7",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.5",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "xlsx": "^0.18.5",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "wrangler": "^4.44.0"
  }
}
